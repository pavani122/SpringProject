<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
 
 <bean class="com.techouts.springCore.Address" name="address1">
 <property name="address">
 <value>Secunderabad</value>
 </property>
 <property name="country">
 <value>India</value>
 </property>
 </bean>
 
 
 <bean class="com.techouts.springCore.Student" name="student">
 <property name="name"><value>Pavani</value></property>
 <property name="id"><value>16</value></property>
 <property name="marks">
 <list>
 <value>71</value>
 <value>88</value>
 <value>63</value>
 <value>91</value>
 <value>79</value>
 </list>
 </property>
 <property name="languages">
 <set>
 <value>C</value>
 <value>C++</value>
 <value>Java</value>
 <value>DBMS</value>
 <value>Spring</value>
 </set>
 </property>
 <property name="map">
 <map>
 <entry key="C" value="71">
 </entry>
 <entry key="C++" value="88">
 </entry>
 <entry key="Java" value="63">
 </entry>
 <entry key="DBMS" value="91">
 </entry>
 <entry key="Spring" value="79">
 </entry>
 </map>
 </property>
 <property name="address" ref="address1" >
 </property>
 </bean>
 </beans>